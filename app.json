{
  "expo": {
    "name": "frontend",
    "slug": "churchease-push",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow churchease to use Face ID."
        }
      ],
      ["expo-router"],
      ["expo-notifications"]
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.churchease.churcheasepush"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "churchease",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8745c92e-f6f7-4b26-b4fb-f549e14cb011"
      }
    }
  }
}
